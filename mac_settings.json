        "sideBarTitle.foreground": "#eeeaea",
        "editor.background": "#1b2022",
        "editor.foreground": "#FFFFFF",
        "sideBarSectionHeader.background": "#2c3237",
        "sideBarSectionHeader.foreground": "#2c3237",
        "tab.activeBackground": "#2c3237",
        "tab.inactiveBackground": "#1b2022",
        "tab.unfocusedActiveForeground": "#ff001e",
        "statusBar.background": "#102F97",
        "statusBar.foreground": "#fff",
        "scrollbarSlider.activeBackground": "#2c3237",
        "scrollbarSlider.hoverBackground": "#2c3237",
        "badge.background": "#1b2022",
        "activityBar.border": "#1b2022",
        "activityBar.background": "#1b2022",
